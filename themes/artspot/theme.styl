  padding: px2em(10px)
  text-align: center
  font-size: 1.25em /* orig 20px */
  font-weight: bold
  text-decoration: inherit
  border-radius: px2em(10px)
  border: px2em(1px) solid rgba(0,0,0,0.2)
  text-shadow: 0 px2em(2px) 0 rgba(0,0,0,0.2)

#jqt .whiteButton
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#fff), to(#eee))
  text-shadow: rgba(255,255,255,0.7) 0 px2em(1px) 0

#jqt .whiteButton.active
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#eee), to(#fff))
  text-shadow: rgba(255,255,255,0.7) 0 px2em(1px) 0

#jqt .grayButton
  color: #fff
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#aaa), to(#888))

#jqt .grayButton.active
  color: #fff
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#888), to(#aaa))

#jqt .redButton
  color: #fff
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#e00), to(#c00))

#jqt .redButton.active
  color: #fff
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#c00), to(#e00))

#jqt .blueButton
  color: #fff
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#00f), to(#00008b))

#jqt .blueButton.active
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#00008b), to(#00f))

#jqt .greenButton
  color: #fff
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#008000), to(#006400))

#jqt .greenButton.active
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#006400), to(#008000))

