  font-size: 0.875em /* orig 14px */
  font-weight: normal

#jqt ul li.img a + small + a + a + a
  margin-left: px2em(0px) !important
  margin-bottom: 0

#jqt ul li a + a
  color: #000
  font-weight: normal
  font-size: 0.875em /* orig 14px */
  text-overflow: ellipsis
  white-space: nowrap
  overflow: hidden
  display: block
  margin: 0
  padding: 0

#jqt ul li a + a + a, #jqt ul li.img a + a + a + a, #jqt ul li.img a + small + a + a + a
  color: #666
  font-weight: normal
  font-size: 0.8125em /* orig 13px */
  margin: 0
  text-overflow: ellipsis
  white-space: nowrap
  overflow: hidden
