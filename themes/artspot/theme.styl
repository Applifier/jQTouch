#jqt h1, #jqt h2, #jqt ul li a
  font-weight: bold
  font-size: 1.125em /* orig 18px */
  text-shadow: #fff 0 px2em(1px) 0
  margin: px2em(10px) px2em(20px) px2em(5px)

#jqt .toolbar
  -webkit-box-sizing: border-box
  padding: px2em(10px)
  height: px2em(45px)
  position: relative
  -webkit-box-shadow: inset 0 px2em(-2px) px2em(2px) rgba(0,0,0,0.2)

#jqt .black-translucent .toolbar
  margin-top: px2em(20px)

#jqt .toolbar > h1
  position: absolute
  overflow: hidden
  left: 50%
  top: px2em(10px)
  line-height: 1em
  margin: px2em(1px) 0 0 px2em(-75px)
  height: px2em(40px)
  font-size: 1.25em /* orig 20px */
  width: px2em(150px)
  font-weight: bold
  text-shadow: rgba(0,0,0,0.5) 0 px2em(1px) px2em(1px)
  text-align: center
  text-overflow: ellipsis
  white-space: nowrap
  color: #fff

#jqt.landscape .toolbar > h1
