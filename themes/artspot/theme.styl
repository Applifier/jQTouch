#jqt .button, #jqt .back, #jqt .cancel, #jqt .add
  position: absolute
  overflow: hidden
  top: px2em(8px, 11px)
  right: px2em(10px, 11px)
  margin: 0
  padding: 0 px2em(8px, 11px)
  width: auto
  height: px2em(30px, 11px)
  line-height: px2em(30px, 11px)
  font-family: inherit
  font-size: 0.6875em /* orig 11px */
  font-weight: bold
  color: #fff
  text-shadow: rgba(0,0,0,0.5) px2em(0px, 11px) px2em(1px, 11px) 0
  text-overflow: ellipsis
  text-decoration: none
  white-space: nowrap
  background: rgba(0,0,0,0.4)
  text-transform: uppercase
  border-radius: px2em(8px, 11px)

#jqt .button.active, #jqt .cancel.active, #jqt .add.active, #jqt .back.active
  background: rgba(255,255,255,0.9)
  text-shadow: none

#jqt .back
  left: px2em(6px, 11px)
  right: auto
  max-width: px2em(41px, 11px)
  padding-left: px2em(11p, 11pxx)
  border-width: 0 0 0 px2em(9px, 11px)
  border-radius: 0 px2em(8px, 11px) px2em(8px, 11px) 0
  -webkit-mask-image: url("img/mask_button_back.svg")
  -webkit-mask-size: px2em(60px, 11px) px2em(30px, 11px)

#jqt .leftButton, #jqt .cancel
  left: px2em(6px, 11px)
  right: auto

